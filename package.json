{
  "name": "boost-ui",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 10.16.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/server.js",
    "start:dev": "razzle start",
    "build": "razzle build",
    "build:analyze": "cross-env BUNDLE_ANALYZE=true razzle build",
    "test": "razzle test --env=jsdom --verbose",
    "test:ci": "razzle test --env=jsdom --silent --runInBand --watchAll=false",
    "lint": "eslint src --max-warnings 0 --ext .js,.jsx",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/src/**/*.styled.js",
      "!<rootDir>/src/styles/**/*.js",
      "!<rootDir>/src/types/*.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@rebass/grid": "^6.1.0",
    "@styled-icons/fa-brands": "^10.0.0",
    "@styled-icons/fa-solid": "^10.0.0",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.2",
    "debounce": "^1.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "razzle": "^3.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.4",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "styled-tools": "^1.7.1",
